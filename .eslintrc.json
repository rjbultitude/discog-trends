{
  "extends": ["airbnb", "prettier"],
  "env": {
    "browser": true,
    "mocha": true,
    "serviceworker": true
  },
  "plugins": ["eslint-plugin-prettier"],
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": 0,
    "prettier/prettier": ["error"]
  }
}
